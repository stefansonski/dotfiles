        HostName sonski-desktop.home
        User stefan
        ForwardX11 yes

Host gbquad
        HostName gbquad.home
        User root

Host pi
        HostName pi.home
        User pi

Host display
        HostName 10.202.20.51
        User root

Host demetre5
