Host *
        ServerAliveInterval 300
        ControlMaster auto
        ControlPath ~/.ssh/controlmasters/%C
        ControlPersist 10m

Host router
        HostName 192.168.1.1
        User admin

Host sonski-desktop
        HostName sonski-desktop.home
        User stefan
        ForwardX11 yes

Host gbquad
        HostName gbquad.home
        User root

Host pi
        HostName pi.home
        User pi

Host *
        Include config.local
